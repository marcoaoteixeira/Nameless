<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <!-- Configurations that affect the Test Framework -->
  <RunConfiguration>
  </RunConfiguration>

  <!-- Parameters used by tests at runtime, can get via TestContext -->
  <TestRunParameters>
    <Parameter name="webAppUrl" value="http://localhost" />
    <Parameter name="webAppUserName" value="Admin" />
    <Parameter name="webAppPassword" value="Password" />
  </TestRunParameters>

  <!-- NUnit Test Adapter Specific section -->
  <NUnit>
    <DefaultTimeout>2500</DefaultTimeout>
	<!-- We'll skip some categories -->
	<Where>cat != 'INTEGRATION'</Where>
  </NUnit>

</RunSettings>