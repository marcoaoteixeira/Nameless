{
  "AppDataProtectionOptions": {
    "ApplicationDiscriminator": null,
    "UseFileSystem": false,
    "FileSystemPath": null
  },

  "HttpContextCorrelationOptions": {
    "Key": "X-Correlation-ID",
    "UseHeader": true
  },

  "JsonWebTokenOptions": {
    "Secret": "1480a572-2424-48a5-b0fd-53314ef3eecb",
    "SecurityAlgorithm": "HS256",
    "AdditionalClaims": [],
    "Issuer": null,
    "ValidateIssuer": false,
    "ValidateIssuerSigninKey": false,
    "Audiences": [],
    "ValidateAudience": false,
    "ValidateLifetime": false,
    "TokenExpiresIn": "00:45:00",
    "ClockSkew": "00:05:00"
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "App_Data/log-.txt",
          "rollingInterval": "Day",
          "rollingOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "ApplicationName": "Identity"
    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
