	<ItemGroup>
		<ProjectReference Include="..\Nameless.Core\Nameless.Core.csproj" />
		<ProjectReference Include="..\Nameless.PubSub.Abstractions\Nameless.PubSub.Abstractions.csproj" />
	</ItemGroup>

</Project>